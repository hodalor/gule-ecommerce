# Production Environment Variables for Gule Admin App

# Backend API URL - Update this with your actual backend URL
REACT_APP_API_URL=https://your-backend-api.herokuapp.com/api

# Admin App Configuration
REACT_APP_APP_NAME=Gule Admin Panel
REACT_APP_VERSION=1.0.0

# Build Configuration
NODE_ENV=production
GENERATE_SOURCEMAP=false
CI=false

# Security Settings
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true

# Feature Flags
REACT_APP_ENABLE_AUDIT_LOGS=true
REACT_APP_ENABLE_BULK_OPERATIONS=true
REACT_APP_ENABLE_EXPORT_FEATURES=true